    {
      "id": "valdir-23-historia-do-mundo-parte-1",
      "title": "Valdir 23 - História do Mundo (Parte 1)",
      "members": "André, Tatá, Pera, Pitty e Léo",
      "published_at": "2019-07-11 13:00:00",
      "thumbnail": "https://i1.sndcdn.com/artworks-000564928187-mjy37n-t3000x3000.jpg",
